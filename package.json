    "firebase-functions": "^3.6.1",
    "node-fetch": "^2.6.0",
    "node-mailjet": "^3.3.1",
    "stripe": "^8.69.0"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.7",
    "@types/node-mailjet": "^3.3.3",
    "firebase-functions-test": "^0.2.0",
